{
    "name": "zenoware/laravel-sql-dump-validator",
    "type": "library",
    "description": "Recursively finds and validates SQL dump (.sql.gz) files inside a directory.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zenoware\\Laravel\\SqlDumpValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "JP Caparas",
            "email": "jp@jpcaparas.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.1",
        "laravel/framework": "^9.0 || ^10.0",
        "symfony/process": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zenoware\\Laravel\\SqlDumpValidator\\Providers\\SqlDumpValidatorServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6"
    }
}
